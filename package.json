{
  "name": "ci-gh-pages",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^6.8.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "standard --fix | snazzy",
    "unit": "react-scripts test --coverage --env=jsdom"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/__tests__/**/*",
      "!src/(App|index|setupTests).js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}
